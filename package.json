{
    "name": "cms-template",
    "version": "0.1.0",
    "private": true,
    "engines": {
        "nodejs": "16.x"
    },
    "scripts": {
        "postinstall": "cd .. && husky install source/.husky",
        "start": "env-cmd -f ./env/.dev craco start",
        "prebuild:dev": "rm -rf build",
        "build:dev": "env-cmd -f ./env/.dev craco build",
        "build:beta": "env-cmd -f ./env/.beta craco build",
        "prebuild:prod": "rm -rf build",
        "build:production": "env-cmd -f ./env/.production craco build",
        "prebuild:staging": "rm -rf build",
        "build:staging": "env-cmd -f ./env/.staging craco build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "lint": "eslint \"src/**/*.js\" --fix",
        "format": "prettier --write \"src/**/*.js\"",
        "i18n": "extract-react-intl-messages -l=en -d en -o src/locales --flat src/**/*.js"
    },
    "dependencies": {
        "@commitlint/cli": "^17.0.0",
        "@commitlint/config-conventional": "^17.3.0",
        "@craco/craco": "^7.0.0",
        "@dnd-kit/core": "^6.0.7",
        "@dnd-kit/sortable": "^7.0.2",
        "@dnd-kit/utilities": "^3.2.1",
        "@jackwilsdon/craco-use-babelrc": "^1.0.0",
        "@tabler/icons-react": "^2.39.0",
        "@testing-library/jest-dom": "^5.16.4",
        "@testing-library/react": "^12.1.5",
        "@testing-library/user-event": "^14.2.0",
        "antd": "^5.10.1",
        "antd-img-crop": "^4.13.0",
        "axios": "^1.2.1",
        "babel-plugin-react-intl-auto": "^3.3.0",
        "buffer": "^6.0.3",
        "classnames": "^2.3.2",
        "dayjs": "^1.11.7",
        "env-cmd": "^10.1.0",
        "eslint-plugin-prettier": "^4.2.1",
        "extract-react-intl-messages": "^4.1.1",
        "husky": "^8.0.2",
        "immer": "^9.0.16",
        "lint-staged": "^13.1.0",
        "lodash": "^4.17.21",
        "moment": "^2.29.4",
        "query-string": "^8.0.3",
        "react": "^17.0.2",
        "react-color": "^2.19.3",
        "react-dom": "^17.0.2",
        "react-icons": "^4.11.0",
        "react-intl": "^6.0.3",
        "react-quill": "^2.0.0",
        "react-redux": "^8.0.5",
        "react-router-dom": "^6.5.0",
        "react-scripts": "5.0.1",
        "recharts": "^2.8.0",
        "redux": "^4.2.0",
        "redux-saga": "^1.2.2",
        "reselect": "^4.1.7",
        "sass": "^1.57.1",
        "unidecode": "^0.1.8",
        "web-vitals": "^2.1.4",
        "yup": "^0.32.11"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "lint-staged": {
        "*.js": [
            "npm run lint",
            "git add ."
        ]
    }
}
